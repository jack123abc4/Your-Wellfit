
<div class="container">
  <div class="header-container">
    <h2 >Welcome, {{name}}!</h2>
  </div>
</div>

 {{#each savedRecipes as |recipe| }}
  <div class="post">
    <h3><a class="recipe-link" href="/recipe/{{recipe.id}}">{{recipe.name}}</a></h3>
    {{!-- <p><small>Calories: {{recipe.calories}}</small></p> --}}
    <p><img class="recipe-image" src="{{recipe.image.image_link}}"></p>
  </div>
   
  {{/each}}


