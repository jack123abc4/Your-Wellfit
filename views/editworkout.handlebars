<nav aria-label="breadcrumb">
  <ol class="breadcrumb">
    <li class="breadcrumb-item"><a href="/workout">Workout</a></li>
    <li class="breadcrumb-item active" aria-current="page">Edit Workout</li>
  </ol>
</nav>

{{#if alert}}
  <div class="alert alert-success alert-dismissible fade show" role="alert">
    {{alert}}
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
  </div>
{{/if}}

<form class="row g-3 needs-validation" id="workout-{{workout.id}}" method="POST" action="/editworkout/{{workout.id}}" novalidate> 
  {{> workout-form}}
</form>

<script src="./js/editWorkout.js"></script>